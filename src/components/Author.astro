---
interface Props {
  prefix: string;
}

const { prefix } = Astro.props;
---

<a class="flex space-x-2 items-center" href="https://twitter.com/kossnocorp">
  <span>{prefix}</span>
  <img
    src="/sasha.jpeg"
    alt="Sasha Koss's avatar"
    class="w-7 h-7 rounded-full"
  />
  <span class="font-medium">Sasha Koss</span>
</a>
